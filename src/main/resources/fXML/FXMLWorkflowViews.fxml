<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<TabPane fx:id="viewsTabPane" style="-fx-tab-min-width: 150; -fx-tab-max-width: 150; -fx-tab-min-height: 75; -fx-tab-max-height: 75;" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <tabs>
    <Tab id="drawTab" closable="false" text="Draw">
         <graphic>
            <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/draw.png" />
               </image>
               <viewport>
                  <Rectangle2D />
               </viewport>
            </ImageView>
         </graphic>
         <content>
            <ScrollPane hbarPolicy="ALWAYS" minHeight="500.0" minWidth="500.0" pannable="true" vbarPolicy="ALWAYS">
              <content>
                <AnchorPane fx:id="drawPane" minHeight="4000.0" minWidth="4000.0" />
              </content>
            </ScrollPane>
         </content>
    </Tab>
    <Tab id="orderTab" closable="false" text="Order">
      <content>
        <AnchorPane fx:id="orderPane" minHeight="500.0" minWidth="500.0" />
      </content>
         <graphic>
            <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/order.png" />
               </image>
               <viewport>
                  <Rectangle2D />
               </viewport>
            </ImageView>
         </graphic>
    </Tab>
  </tabs>
</TabPane>
